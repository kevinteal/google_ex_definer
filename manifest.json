{
  "name": "Definer Of Words",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Defines Words You Highlight Maybe, I'm Writing This Before I Make The App.",
  "browser_action": {
    "default_icon": "off.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
 "permissions": [
    "tabs", "http://*/*", "https://*/*"
],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["jquery-2.1.4.min.js"]
  }
]
}